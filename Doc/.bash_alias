# aliases
alias reloadbash="source /e/GitHub/MyConfigFiles/Doc/.bashrc"

alias pss="ps aux | grep ServerLauncher"
alias psn="ps aux | grep node -w"
alias psm="ps aux | grep mongod -w"
alias psr="ps aux | grep redis -w"
alias psmr="psm; psr"

alias cdg="cd $GAME_MAJOR"

alias sshvm="ssh will@UbuntuVM"

alias sshgitlab="ssh slggitlab@10.12.7.92"
alias sshdev="ssh slgdev@10.12.7.93"

alias ....="cd ../../.."
alias .....="cd ../../../.."
alias cd-="cd -"
alias l="ls"
alias la="ls -lah"
alias ll="la"
alias lm='ll | more'
alias lsd='ls -d *(/)'
alias df='df -h'
alias du='du -h -d1'
alias cp='cp -iR'

alias mv='mv -i'
alias rm='rm -ivr'
alias md='mkdir -p'
alias ztar='tar -czvf'
alias uztar='tar -xzvf'
alias ubz2tar='tar -jvxf'
alias ttar='tar -tvf'
alias man='man -a'
alias vi='vim'
alias mvi='gvim'

alias gitk='/usr/local/git/bin/gitk'
alias gk='gitk'
alias ga='git add --ignore-removal'
alias gsu='git submodule update'


alias rc='redis-cli'

alias myip='ipconfig | grep inet\ '

alias j='java'
alias m='mvn'
alias mp='mvn package'

alias rmongo='cd /tmp; mkdir -p /tmp/log; sleep 1; pkill mongo; sleep 2; rm -rf /data/db/mongod.lock; sleep 2; mongod --dbpath /data/db > /tmp/log/mongo.log&; sleep 1; cd -'
alias rredis='cd /tmp; mkdir -p /tmp/log; sleep 1; pkill redis; sleep 2; redis-server > /tmp/log/redis.log&; sleep 1; cd -'
alias rdb='rmongo; sleep 2; rredis;'

